    "zod": "^3.23.2",
    "@prisma/client": "^5.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.4.2",
    "prisma": "^5.6.0",
    "supertest": "^6.4.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3",
    "eslint": "^8.47.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0"
